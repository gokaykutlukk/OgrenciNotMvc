@using OgrenciNotMvc.Models.EntityFramework
@model List<TBLNOTLAR>
@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<table class=" table table-bordered">
    <tr>
        <th>NotId</th>
        <th>DersId</th>
        <th>Öğrenci Id</th>
        <th>Sınav1</th>
        <th>Sınav2</th>
        <th>Sınav3</th>
        <th>Proje</th>
        <th>Ortalama</th>
        <th>Durum</th>
    </tr>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>@item.NOTID</td>
                <td>@item.DERSID</td>
                <td>@item.OGRID</td>
                <td>@item.SINAV1</td>
                <td>@item.SINAV2</td>
                <td>@item.SINAV3</td>
                <td>@item.PROJE</td>
                <td>@item.ORTALAMA</td>
                <td>@item.DURUM</td>
            </tr>
        }
</table>

