@using OgrenciNotMvc.Models.EntityFramework
@model List<TBLKULUPLER>
@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<table class="table table-bordered">
    <tr>
        <th>Kulüp Id</th>
        <th>Kulüp Adı</th>
    </tr>
    <tbody>
        @foreach(var item in Model)
            {
                <tr>
                    <th>@item.KULUPID</th>
                    <th>@item.KULUPAD</th>
                </tr>
            }
    </tbody>
</table>
<a href="/Club/NewClub" class="btn btn-primary">Yeni klub Kaydı</a>

