@using OgrenciNotMvc.Models.EntityFramework
@model List<TBLKULUPLER>
@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<table class="table table-bordered">
    <tr>
        <th>Kulüp Id</th>
        <th>Kulüp Adı</th>
        <th>İŞLEMLER</th>
    </tr>
    <tbody>
        @foreach(var item in Model)
            {
                <tr>
                    <td>@item.KULUPID</td>
                    <td>@item.KULUPAD</td>
                    <td><a href="/Club/Delete/@item.KULUPID" class="btn btn-danger">Sil</a>
                    <a href="/Club/Update/@item.KULUPID" class="btn btn-success">Güncelleme</a></td>
                </tr>
            }
    </tbody>
</table>
<a href="/Club/NewClub" class="btn btn-primary">Yeni klub Kaydı</a>

